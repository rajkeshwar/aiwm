{
    "rewrites": [
        {
            "source": "/p/(.*)",
            "destination": "https://dev.qawmai.com/$1"
        },
        {
            "source": "/xyz/(.*)",
            "destination": "https://jsonplaceholder.typicode.com/$1"
        },
        {
            "source": "/api/(.*)",
            "destination": "https://dev.qawmai.com/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}
